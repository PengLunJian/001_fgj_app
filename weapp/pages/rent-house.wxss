.container .content {
  height: 100vh;
  padding-top: 210rpx;
}
.container .content .header .tab-bar {
  display: flex;
  flex-wrap: wrap;
  position: fixed;
  top: 130rpx;
  left: 0;
  z-index: 1000;
  background-color: #ffffff;
  box-shadow: 0 5px 5px rgba(0, 0, 0, 0.03);
  font-size: 28rpx;
  text-align: center;
  color: #333333;
}
.container .content .header .tab-bar .tab-item {
  width: 33.333%;
  position: relative;
  padding: 20rpx 0;
}
.container .content .header .tab-bar .tab-item .tab-text {
  height: 40rpx;
  line-height: 40rpx;
  border-right: 0.5px solid #dedede;
  font-size: 0;
}
.container .content .header .tab-bar .tab-item .tab-text .tab-label,
.container .content .header .tab-bar .tab-item .tab-text .tab-triangle {
  display: inline-block;
  vertical-align: middle;
}
.container .content .header .tab-bar .tab-item .tab-text .tab-label.tab-label,
.container .content .header .tab-bar .tab-item .tab-text .tab-triangle.tab-label {
  font-size: 28rpx;
}
.container .content .header .tab-bar .tab-item .tab-text .tab-label.tab-triangle,
.container .content .header .tab-bar .tab-item .tab-text .tab-triangle.tab-triangle {
  margin-left: 20rpx;
  border-top: 12rpx solid #333333;
  border-left: 8rpx solid transparent;
  border-right: 8rpx solid transparent;
  transition: all 300ms ease;
}
.container .content .header .tab-bar .tab-item.active {
  color: #FF7340;
}
.container .content .header .tab-bar .tab-item.active .tab-triangle {
  transform: rotate(180deg);
  border-top-color: #FF7340 !important;
}
.container .content .header .tab-bar .tab-item:last-child .tab-text {
  border: none;
}
.container .content .body {
  height: 100%;
}
.container .content .body > .scroll-view {
  height: 100%;
}
.container .content .body > .scroll-view .item {
  padding: 0 30rpx;
  background-color: #ffffff;
}
.container .content .body > .scroll-view .item .item-inner {
  position: relative;
  padding: 30rpx 0;
  border-bottom: 0.5px solid #f0f0f0;
}
.container .content .body > .scroll-view .item .item-inner .item-img {
  position: absolute;
  top: 0;
  left: 0;
  width: 220rpx;
  height: 240rpx;
  padding: 30rpx 0;
}
.container .content .body > .scroll-view .item .item-inner .item-img image {
  width: 100%;
  height: 100%;
}
.container .content .body > .scroll-view .item .item-inner .item-info {
  padding-left: 240rpx;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row {
  margin-bottom: 8rpx;
  font-size: 0;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row:last-child {
  margin: 0;
  padding-top: 4rpx;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-title {
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-addr,
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-split,
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-size {
  height: 32rpx;
  line-height: 32rpx;
  display: inline-block;
  vertical-align: middle;
  font-size: 24rpx;
  color: #666666;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-addr.item-split,
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-split.item-split,
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-size.item-split {
  padding: 6rpx 10rpx;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-addr.item-split:before,
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-split.item-split:before,
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-size.item-split:before {
  content: '';
  display: block;
  width: 0;
  height: 100%;
  border-left: 1px solid #dedede;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-value,
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-unit {
  height: 100%;
  display: inline-block;
  vertical-align: middle;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-value.item-value,
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-unit.item-value {
  font-size: 32rpx;
  font-weight: bold;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-value.item-unit,
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-unit.item-unit {
  font-size: 24rpx;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-normal {
  height: 40rpx;
  line-height: 40rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
  font-size: 24rpx;
  color: #999999;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-normal.item-price {
  color: #F40505;
  font-size: 0;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-normal:last-child {
  margin: 0;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-tag {
  height: 40rpx;
  line-height: 36rpx;
  display: inline-block;
  vertical-align: middle;
  padding: 0 10rpx;
  margin-right: 10rpx;
  border: 1px solid #ff7340;
  font-size: 24rpx;
  color: #ff7340;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-tag.item-state {
  border: none;
  background-color: #009966;
  line-height: 40rpx;
  color: #ffffff;
}
.container .content .body > .scroll-view .item .item-inner .item-info .item-row .item-tag:last-child {
  margin: 0;
}
.container .content .body > .scroll-view .item:last-child .item-inner {
  border: none;
}
.container .content .footer .modal .modal-bg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 998;
  transition: all 300ms ease-out;
  background-color: rgba(0, 0, 0, 0.5);
}
.container .content .footer .modal .modal-content {
  position: fixed;
  top: 210rpx;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 600rpx;
  background-color: #ffffff;
  transition: all 300ms ease-out;
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.15);
}
.container .content .footer .modal .modal-content .modal-body {
  height: 100%;
}
.container .content .footer .modal .modal-content .modal-body > .scroll-view {
  height: 100%;
}
.container .content .footer .modal .modal-content .modal-body .modal-group {
  padding: 0 30rpx;
}
.container .content .footer .modal .modal-content .modal-body .modal-group .modal-block {
  padding: 30rpx 0;
}
.container .content .footer .modal .modal-content .modal-body .modal-group .modal-block.border {
  border-bottom: 0.5px solid #dedede;
}
.container .content .footer .modal .modal-content .modal-body .modal-group .modal-row {
  margin-left: -15rpx;
  margin-right: -15rpx;
}
.container .content .footer .modal .modal-content .modal-body .modal-group .min-price,
.container .content .footer .modal .modal-content .modal-body .modal-group .max-price {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #666666;
}
.container .content .footer .modal .modal-content .modal-body .modal-group .min-price.max-price,
.container .content .footer .modal .modal-content .modal-body .modal-group .max-price.max-price {
  text-align: right;
}
.container .content .footer .modal .modal-content .modal-body .modal-group .modal-col {
  padding: 15rpx;
}
.container .content .footer .modal .modal-content .modal-body .modal-group .modal-col .btn-price {
  overflow: hidden;
  height: 60rpx;
  line-height: 56rpx;
  border-radius: 60rpx;
  border: 1px solid #dedede;
  font-size: 24rpx;
  text-align: center;
  color: #999999;
}
.container .content .footer .modal .modal-content .modal-body .modal-group .modal-col.active .btn-price {
  border: none;
  line-height: 60rpx;
  background: -webkit-linear-gradient(left, #FFBB1D, #F2890B);
  color: #ffffff;
}
.container .content .footer .modal .modal-content .modal-body .modal-sort .btn-sort {
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 0.5px solid #dedede;
  font-size: 28rpx;
  text-align: center;
  color: #666666;
}
.container .content .footer .modal .modal-content .modal-body .modal-sort .btn-sort:last-child {
  border: none;
}
.container .content .footer .modal .modal-content .modal-footer .modal-button {
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 1000;
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  text-align: center;
}
.container .content .footer .modal .modal-content .modal-footer .modal-button .btn {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  letter-spacing: 1px;
  color: #ffffff;
}
.container .content .footer .modal .modal-content .modal-footer .modal-button .btn.btn-clear {
  width: 35%;
  background-color: #ffffff;
  border-top: 0.5px solid #dedede;
  color: #666666;
}
.container .content .footer .modal .modal-content .modal-footer .modal-button .btn.btn-confirm {
  width: 65%;
  background: -webkit-linear-gradient(left, #FFBB1D, #F2890B);
}
.container .content .footer .modal:nth-child(1) .modal-content,
.container .content .footer .modal:nth-child(2) .modal-content {
  padding-bottom: 100rpx;
}
.container .content .footer .modal:nth-child(3) .modal-content {
  height: 500rpx;
}
.container .content .footer .modal.hide .modal-bg {
  opacity: 0;
  visibility: hidden;
}
.container .content .footer .modal.hide .modal-content {
  visibility: hidden;
  transform: translateY(-600rpx);
}
.container .content .footer .modal.hide:nth-child(3) .modal-content {
  transform: translateY(-500rpx);
}

