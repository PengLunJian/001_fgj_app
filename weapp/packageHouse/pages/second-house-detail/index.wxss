/* iPhone X, iPhone XS */
@media only screen and (device-width: 375px) and (max-device-height: 812px) and (-webkit-device-pixel-ratio: 3) {
  .top-bar {
    padding-top: 44px !important;
  }
  .context {
    padding-top: 89px !important;
  }
}
/* iPhone XR */
@media only screen and (device-width: 414px) and (max-device-height: 896px) and (-webkit-device-pixel-ratio: 2) {
  .top-bar {
    padding-top: 44px !important;
  }
  .context {
    padding-top: 89px !important;
  }
}
/* iPhone XS Max */
@media only screen and (device-width: 414px) and (max-device-height: 896px) and (-webkit-device-pixel-ratio: 3) {
  .top-bar {
    padding-top: 44px !important;
  }
  .context {
    padding-top: 89px !important;
  }
}
/* iPhone 6, iPhone XS */
@media only screen and (device-width: 414px) and (max-device-height: 736px) and (-webkit-device-pixel-ratio: 3) {
  .top-bar {
    padding-top: 20px !important;
  }
  .context {
    padding-top: 65px !important;
  }
}
@media only screen and (device-width: 360px) and (max-device-height: 780px) and (-webkit-device-pixel-ratio: 3) {
  .top-bar {
    padding-top: 28px !important;
  }
  .context {
    padding-top: 73px !important;
  }
}
.container {
  min-height: 100vh;
}
.container .content {
  height: 100vh;
}
.container .content .body {
  height: 100%;
  position: relative;
}
.container .content .body .context {
  height: 100%;
  padding-top: 65px;
  padding-bottom: 110rpx;
}
.container .content .body .context .scroll-view {
  height: 100%;
}
.container .content .body .context .fixed-bar {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 1000;
  background-color: #ffffff;
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.05);
}
.container .content .body .context .fixed-bar .fixed-bar-left {
  display: flex;
  width: 36%;
  height: 110rpx;
  line-height: 110rpx;
  padding: 0 20rpx;
  text-align: center;
  font-size: 0;
}
.container .content .body .context .fixed-bar .fixed-bar-left .btn {
  width: 50%;
  padding-top: 18rpx;
  line-height: 40rpx;
  color: #999999;
}
.container .content .body .context .fixed-bar .fixed-bar-left .btn .btn-icon {
  display: block;
  font-size: 40rpx;
}
.container .content .body .context .fixed-bar .fixed-bar-left .btn .btn-text {
  display: block;
  font-size: 24rpx;
}
.container .content .body .context .fixed-bar .fixed-bar-left .btn.btn-favorite .btn-icon:before {
  content: '\e633';
}
.container .content .body .context .fixed-bar .fixed-bar-left .btn.btn-favorite .btn-icon.active:before {
  content: '\e63c';
}
.container .content .body .context .fixed-bar .fixed-bar-right {
  width: 64%;
  display: flex;
  font-size: 0;
}
.container .content .body .context .fixed-bar .fixed-bar-right .btn {
  height: 110rpx;
  line-height: 110rpx;
  text-align: center;
  color: #ffffff;
}
.container .content .body .context .fixed-bar .fixed-bar-right .btn.btn-msg {
  background-color: #FFCC00;
}
.container .content .body .context .fixed-bar .fixed-bar-right .btn.btn-tel {
  background-color: #FF9900;
}
.container .content .body .context .fixed-bar .fixed-bar-right .btn .btn-icon {
  display: inline-block;
  vertical-align: middle;
  height: 110rpx;
  line-height: 110rpx;
  margin-right: 10rpx;
  font-size: 46rpx;
}
.container .content .body .context .fixed-bar .fixed-bar-right .btn .btn-text {
  display: inline-block;
  vertical-align: middle;
  height: 110rpx;
  line-height: 110rpx;
  font-size: 36rpx;
  letter-spacing: 1px;
}

