.container {
  position: relative;
  min-height: 100vh;
}
.container .content {
  height: 100vh;
  padding-top: 130rpx;
  padding-bottom: 110rpx;
}
.container .content .body {
  height: 100%;
}
.container .content .body .scroll-view {
  height: 100%;
}
.container .content .body .scroll-view .module {
  position: relative;
  background-color: transparent;
}
.container .content .body .scroll-view .module:first-child {
  margin: 0;
}
.container .content .body .scroll-view .module:last-child {
  margin: 0;
  padding-bottom: 20rpx;
}
.container .content .body .scroll-view .module .module-content {
  background-color: #ffffff;
}
.container .content .body .scroll-view .module .module-content .module-header {
  padding: 30rpx;
  border-bottom: 0.5px solid #F0F0F0;
}
.container .content .body .scroll-view .module .module-content .module-header .module-title {
  height: 30rpx;
  line-height: 30rpx;
  padding-left: 20rpx;
  border-left: 4px solid #FF9900;
  font-size: 32rpx;
  font-weight: bold;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context {
  min-height: 400rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .swiper {
  height: 520rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .swiper .swiper-img {
  width: 100%;
  height: 100%;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .swiper-code {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 20rpx;
  text-align: center;
  font-size: 0;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .swiper-code .swiper-text {
  height: 40rpx;
  line-height: 40rpx;
  min-width: 100rpx;
  display: inline-block;
  padding: 0 20rpx;
  border-radius: 40rpx;
  background-color: rgba(0, 0, 0, 0.6);
  font-size: 24rpx;
  color: #ffffff;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .btn-favorite,
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .btn-share {
  width: 80rpx;
  height: 80rpx;
  overflow: hidden;
  border-radius: 80rpx;
  line-height: 80rpx;
  position: absolute;
  bottom: -40rpx;
  z-index: 1;
  background-color: #ffffff;
  text-align: center;
  color: #999999;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .btn-favorite.btn-favorite,
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .btn-share.btn-favorite {
  right: 125rpx;
  color: #ff5500;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .btn-favorite.btn-share,
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .btn-share.btn-share {
  right: 30rpx;
  color: #00EE00;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .btn-favorite .iconfont,
.container .content .body .scroll-view .module .module-content .module-body .module-context .banner .btn-share .iconfont {
  font-size: 40rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .product {
  padding: 30rpx 0;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .product .row {
  line-height: 40rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .product .row .col-12 .product-title {
  line-height: 60rpx;
  padding: 0 30rpx;
  margin-bottom: 30rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: #333333;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .product .row .col-4 {
  padding-bottom: 20rpx;
  text-align: center;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .product .row .col-4 .value {
  margin-bottom: 20rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: #F40505;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .product .row .col-4 .label {
  font-size: 24rpx;
  color: #999999;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .product .row .col-6 {
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .product .row .col-6 .label {
  margin-right: 20rpx;
  color: #999999;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .items {
  padding-bottom: 30rpx;
  text-align: center;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .items .item {
  width: 20%;
  position: relative;
  padding-top: 30rpx;
  color: #333333;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .items .item .item-icon {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 60rpx;
  margin-bottom: 10rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .items .item .item-label {
  display: block;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .map-container {
  padding: 30rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .map-container map {
  width: 100%;
  height: 400rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box {
  padding: 20rpx 0;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item {
  padding: 0 30rpx;
  font-size: 0;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-inner {
  position: relative;
  padding: 20rpx 0;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-img {
  width: auto;
  display: inline-block;
  vertical-align: middle;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-img image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 100%;
  background-color: #ffffff;
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.05);
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info {
  width: auto;
  display: inline-block;
  vertical-align: middle;
  line-height: 40rpx;
  padding-left: 30rpx;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-name {
  font-size: 32rpx;
  color: #333333;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-phone {
  font-size: 28rpx;
  color: #999999;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-btns {
  width: auto;
  position: absolute;
  top: 0;
  right: 0;
  padding: 20rpx 0;
  font-size: 0;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-btns .btn-icon {
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 30rpx;
  font-size: 46rpx;
  text-align: center;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-btns .btn-icon.icon-msg {
  color: #FFBB00;
}
.container .content .body .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-btns .btn-icon.icon-tel {
  margin: 0;
  color: #00EE00;
}
.container .content .body .fixed-bar {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 1000;
  background-color: #ffffff;
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.05);
}
.container .content .body .fixed-bar .agent {
  width: 36%;
  padding-left: 30rpx;
  text-align: left;
  font-size: 0;
}
.container .content .body .fixed-bar .agent .agent-img {
  width: 70rpx;
  height: 70rpx;
  display: inline-block;
  vertical-align: middle;
}
.container .content .body .fixed-bar .agent .agent-img image {
  width: 100%;
  height: 100%;
  border-radius: 100%;
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.05);
}
.container .content .body .fixed-bar .agent .agent-name {
  width: auto;
  height: 110rpx;
  line-height: 110rpx;
  display: inline-block;
  vertical-align: middle;
  margin-left: 10rpx;
  font-size: 28rpx;
  color: #333333;
}
.container .content .body .fixed-bar .btns {
  width: 64%;
  display: flex;
  font-size: 0;
}
.container .content .body .fixed-bar .btns .btn {
  height: 110rpx;
  line-height: 110rpx;
  text-align: center;
  color: #ffffff;
}
.container .content .body .fixed-bar .btns .btn.btn-msg {
  background: -webkit-linear-gradient(left, #FFBB00, #FF9900);
}
.container .content .body .fixed-bar .btns .btn.btn-tel {
  background: -webkit-linear-gradient(left, #55DD66, #00EE00);
}
.container .content .body .fixed-bar .btns .btn .btn-icon {
  display: inline-block;
  vertical-align: middle;
  height: 110rpx;
  line-height: 110rpx;
  margin-right: 10rpx;
  font-size: 40rpx;
}
.container .content .body .fixed-bar .btns .btn .btn-text {
  display: inline-block;
  vertical-align: middle;
  height: 110rpx;
  line-height: 110rpx;
  font-size: 36rpx;
  letter-spacing: 1px;
}

