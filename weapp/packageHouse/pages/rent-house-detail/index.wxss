.container {
  min-height: 100vh;
}
.container .content {
  height: 100vh;
}
.container .content .body {
  height: 100%;
  position: relative;
}
.container .content .body .context {
  height: 100%;
  padding-top: 65px;
  padding-bottom: 110rpx;
}
.container .content .body .context .scroll-view {
  height: 100%;
}
.container .content .body .context .scroll-view .module {
  position: relative;
  background-color: transparent;
}
.container .content .body .context .scroll-view .module:first-child {
  margin: 0;
}
.container .content .body .context .scroll-view .module:last-child {
  margin: 0;
  padding-bottom: 20rpx;
}
.container .content .body .context .scroll-view .module .module-content {
  background-color: #ffffff;
}
.container .content .body .context .scroll-view .module .module-content .module-header {
  padding: 30rpx 30rpx 0;
}
.container .content .body .context .scroll-view .module .module-content .module-header .module-title {
  height: 32rpx;
  line-height: 32rpx;
  padding-left: 20rpx;
  border-left: 4px solid #FF9900;
  font-size: 32rpx;
  letter-spacing: 0.5px;
  font-weight: bold;
  color: #333333;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context {
  /*min-height: unit(400, rpx);*/
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .swiper {
  height: 520rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .swiper .swiper-img {
  width: 100%;
  height: 100%;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .swiper-code {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 30rpx;
  text-align: center;
  font-size: 0;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .swiper-code .swiper-text {
  height: 44rpx;
  line-height: 44rpx;
  min-width: 90rpx;
  display: inline-block;
  padding: 0 20rpx;
  border-radius: 44rpx;
  background-color: rgba(0, 0, 0, 0.5);
  font-size: 24rpx;
  color: #ffffff;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .btn-favorite {
  width: 90rpx;
  height: 90rpx;
  border-radius: 100%;
  position: absolute;
  right: 40rpx;
  bottom: -60rpx;
  z-index: 1;
  overflow: hidden;
  background-color: #ffffff;
  text-align: center;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .btn-favorite .btn-icon {
  height: 60rpx;
  line-height: 70rpx;
  font-size: 40rpx;
  color: #F40505;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .btn-favorite .btn-icon:before {
  content: '\e633';
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .btn-favorite .btn-icon.active:before {
  content: '\e63c';
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .btn-favorite .btn-text {
  height: 30rpx;
  line-height: 30rpx;
  font-size: 22rpx;
  color: #999999;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .btn-share {
  position: absolute;
  top: 100rpx;
  right: -30rpx;
  z-index: 1;
  overflow: hidden;
  border-radius: 60rpx;
  padding: 0 50rpx 0 20rpx;
  background: -webkit-linear-gradient(left, rgba(255, 153, 0, 0.8), rgba(255, 204, 0, 0.8));
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.05);
  text-align: left;
  font-size: 0;
  color: #ffffff;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .btn-share .btn-icon {
  height: 60rpx;
  display: inline-block;
  vertical-align: middle;
  line-height: 60rpx;
  font-size: 32rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .banner .btn-share .btn-text {
  height: 60rpx;
  display: inline-block;
  vertical-align: middle;
  padding-left: 4rpx;
  line-height: 60rpx;
  font-size: 26rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .product {
  padding: 30rpx 0;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .product .row {
  line-height: 40rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .product .row .col-12 .product-title {
  line-height: 60rpx;
  padding: 0 30rpx;
  margin-bottom: 30rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: #333333;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .product .row .col-4 {
  padding-bottom: 20rpx;
  text-align: center;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .product .row .col-4 .value {
  margin-bottom: 20rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: #F40505;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .product .row .col-4 .label {
  font-size: 24rpx;
  color: #999999;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .product .row .col-6 {
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .product .row .col-6 .label {
  margin-right: 20rpx;
  color: #999999;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .items {
  padding-bottom: 30rpx;
  text-align: center;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .items .item {
  width: 20%;
  position: relative;
  padding-top: 30rpx;
  color: #333333;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .items .item .item-icon {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 60rpx;
  margin-bottom: 10rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .items .item .item-label {
  display: block;
  height: 40rpx;
  line-height: 40rpx;
  text-decoration: line-through;
  font-size: 24rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .items .item.on .item-label {
  text-decoration: none;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .map-container {
  padding: 30rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .map-container .map-content {
  position: relative;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .map-container .map-content .map-canvas {
  width: 100%;
  height: 400rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .map-container .map-content .map-marker {
  position: absolute;
  top: calc(50% - 40rpx);
  left: 50%;
  transform: translate3d(-50%, -50%, 0);
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .map-container .map-content .map-marker .marker-container .marker-content {
  position: relative;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .map-container .map-content .map-marker .marker-container .marker-content .marker-text {
  position: relative;
  z-index: 2;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 30rpx;
  background-color: #ffffff;
  border-radius: 10rpx;
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.05);
  letter-spacing: 0.5px;
  font-size: 28rpx;
  color: #333333;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .map-container .map-content .map-marker .marker-container .marker-content .marker-icon {
  width: 0;
  height: 0;
  position: absolute;
  left: 50%;
  bottom: -20rpx;
  z-index: 2;
  transform: translateX(-50%);
  border-top: 20rpx solid #ffffff;
  border-left: 20rpx solid transparent;
  border-right: 20rpx solid transparent;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .map-container .map-content .map-marker .marker-container .marker-content .marker-circle {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 50%;
  bottom: -40rpx;
  transform: translateX(-50%);
  border-radius: 100%;
  border: 1px solid #ff9900;
  background-color: rgba(255, 153, 0, 0.5);
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box {
  padding: 20rpx 0;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item {
  padding: 0 30rpx;
  font-size: 0;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-inner {
  position: relative;
  padding: 20rpx 0;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-img {
  width: auto;
  display: inline-block;
  vertical-align: middle;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-img image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 100%;
  background-color: #ffffff;
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.05);
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info {
  width: auto;
  display: inline-block;
  vertical-align: middle;
  line-height: 40rpx;
  padding-left: 30rpx;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-name {
  font-size: 32rpx;
  color: #333333;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-phone {
  font-size: 28rpx;
  color: #999999;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-btns {
  width: auto;
  position: absolute;
  top: 0;
  right: 0;
  padding: 20rpx 0;
  font-size: 0;
}
.container .content .body .context .scroll-view .module .module-content .module-body .module-context .agent-box .agent-item .agent-info .agent-btns .btn-icon {
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  display: inline-block;
  vertical-align: middle;
  border-radius: 100%;
  margin-left: 40rpx;
  background-color: rgba(255, 153, 0, 0.05);
  font-size: 40rpx;
  text-align: center;
  color: #FF9900;
}
.container .content .body .context .fixed-bar {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 1000;
  background-color: #ffffff;
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.05);
}
.container .content .body .context .fixed-bar .agent {
  width: 36%;
  padding-left: 30rpx;
  text-align: left;
  font-size: 0;
}
.container .content .body .context .fixed-bar .agent .agent-img {
  width: 70rpx;
  height: 70rpx;
  display: inline-block;
  vertical-align: middle;
}
.container .content .body .context .fixed-bar .agent .agent-img image {
  width: 100%;
  height: 100%;
  border-radius: 100%;
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.05);
}
.container .content .body .context .fixed-bar .agent .agent-name {
  width: auto;
  height: 110rpx;
  line-height: 110rpx;
  display: inline-block;
  vertical-align: middle;
  margin-left: 10rpx;
  font-size: 28rpx;
  color: #333333;
}
.container .content .body .context .fixed-bar .btns {
  width: 64%;
  display: flex;
  font-size: 0;
}
.container .content .body .context .fixed-bar .btns .btn {
  height: 110rpx;
  line-height: 110rpx;
  text-align: center;
  color: #ffffff;
}
.container .content .body .context .fixed-bar .btns .btn.btn-msg {
  background-color: #FFCC00;
}
.container .content .body .context .fixed-bar .btns .btn.btn-tel {
  background-color: #FF9900;
}
.container .content .body .context .fixed-bar .btns .btn .btn-icon {
  display: inline-block;
  vertical-align: middle;
  height: 110rpx;
  line-height: 110rpx;
  margin-right: 10rpx;
  font-size: 46rpx;
}
.container .content .body .context .fixed-bar .btns .btn .btn-text {
  display: inline-block;
  vertical-align: middle;
  height: 110rpx;
  line-height: 110rpx;
  font-size: 36rpx;
  letter-spacing: 1px;
}

