page,
view,
icon,
text,
label,
image {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  -webkit-tap-highlight-color: transparent;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
view {
  display: block;
}
.row {
  display: flex;
  flex-wrap: wrap;
}
.col-3 {
  width: 25%;
}
.col-4 {
  width: 33.333%;
}
.col-6 {
  width: 50%;
}
.col-12 {
  width: 100%;
}
image {
  display: block;
}
page {
  font-size: 26rpx;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  background-color: #fafafa;
}
button {
  padding: 0;
  border: none;
  border-radius: 0;
  padding-right: 0;
  background-color: transparent;
}
button:focus {
  outline: none;
}
button:after {
  display: none;
}
.module {
  margin-bottom: 20rpx;
  background-color: #ffffff;
}
.module:last-child {
  margin: 0;
}
.btn {
  position: relative;
}
.btn:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  visibility: hidden;
  transition: all 150ms ease;
}
.btn:active:before {
  visibility: visible;
  background-color: rgba(0, 0, 0, 0.3);
}
.ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.fade {
  animation: fade 300ms ease-out;
}
.fade.hide {
  animation: hide 300ms ease-out forwards;
}
@keyframes fade {
  0% {
    opacity: 0;
    visibility: hidden;
  }
  100% {
    opacity: 1;
    visibility: visible;
  }
}
@keyframes hide {
  0% {
    opacity: 1;
    visibility: visible;
  }
  100% {
    opacity: 0;
    visibility: hidden;
  }
}
@keyframes rotate {
  0% {
    transform: rotate(0deg);
  }
  50% {
    transform: rotate(180deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
.banner {
  position: relative;
}
.banner .banner-swiper .swiper {
  height: 280rpx;
}
.banner .banner-swiper .swiper-img {
  width: 100%;
  height: 280rpx;
}
.banner .banner-code {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 20rpx;
  text-align: center;
  font-size: 0;
}
.banner .banner-code .swiper-code {
  width: 20px;
  height: 3px;
  margin-right: 6px;
  display: inline-block;
  vertical-align: middle;
  background-color: #ffffff;
  transition: all 300ms ease-out;
}
.banner .banner-code .swiper-code:last-child {
  margin: 0;
}
.banner .banner-code .swiper-code.active {
  background-color: #FF9900;
}

