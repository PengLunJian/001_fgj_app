.item {
  padding: 0 30rpx;
  background-color: #ffffff;
}
.item .item-inner {
  position: relative;
  padding: 30rpx 0;
  border-bottom: 0.5px solid #F0F0F0;
}
.item .item-inner .item-img {
  position: absolute;
  top: 0;
  left: 0;
  width: 220rpx;
  height: 240rpx;
  padding: 30rpx 0;
}
.item .item-inner .item-img image {
  width: 100%;
  height: 100%;
}
.item .item-inner .item-info {
  padding-left: 240rpx;
}
.item .item-inner .item-info .item-row {
  margin-bottom: 8rpx;
  font-size: 0;
}
.item .item-inner .item-info .item-row:last-child {
  margin: 0;
  padding-top: 4rpx;
}
.item .item-inner .item-info .item-row .item-title {
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
}
.item .item-inner .item-info .item-row .item-addr,
.item .item-inner .item-info .item-row .item-split,
.item .item-inner .item-info .item-row .item-size {
  height: 32rpx;
  line-height: 32rpx;
  display: inline-block;
  vertical-align: middle;
  font-size: 24rpx;
  color: #666666;
}
.item .item-inner .item-info .item-row .item-addr.item-split,
.item .item-inner .item-info .item-row .item-split.item-split,
.item .item-inner .item-info .item-row .item-size.item-split {
  padding: 6rpx 10rpx;
}
.item .item-inner .item-info .item-row .item-addr.item-split:before,
.item .item-inner .item-info .item-row .item-split.item-split:before,
.item .item-inner .item-info .item-row .item-size.item-split:before {
  content: '';
  display: block;
  width: 0;
  height: 100%;
  border-left: 1px solid #DEDEDE;
}
.item .item-inner .item-info .item-row .item-value,
.item .item-inner .item-info .item-row .item-unit {
  height: 100%;
  display: inline-block;
  vertical-align: middle;
}
.item .item-inner .item-info .item-row .item-value.item-value,
.item .item-inner .item-info .item-row .item-unit.item-value {
  font-size: 32rpx;
  font-weight: bold;
}
.item .item-inner .item-info .item-row .item-value.item-unit,
.item .item-inner .item-info .item-row .item-unit.item-unit {
  font-size: 24rpx;
}
.item .item-inner .item-info .item-row .item-normal {
  height: 40rpx;
  line-height: 40rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
  font-size: 24rpx;
  color: #999999;
}
.item .item-inner .item-info .item-row .item-normal.item-price {
  color: #F40505;
  font-size: 0;
}
.item .item-inner .item-info .item-row .item-normal:last-child {
  margin: 0;
}
.item .item-inner .item-info .item-row .item-tag {
  height: 40rpx;
  line-height: 36rpx;
  display: inline-block;
  vertical-align: middle;
  padding: 0 10rpx;
  margin-right: 10rpx;
  border: 1px solid #FF9900;
  font-size: 24rpx;
  color: #FF9900;
}
.item .item-inner .item-info .item-row .item-tag.item-state {
  border: none;
  background-color: #009966;
  line-height: 40rpx;
  color: #ffffff;
}
.item .item-inner .item-info .item-row .item-tag:last-child {
  margin: 0;
}
second-house-item:last-child .item-inner {
  border: none !important;
}

