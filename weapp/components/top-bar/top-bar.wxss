.top-bar {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background-color: #ffffff;
  padding-top: 30rpx;
}
.top-bar .top-inner {
  position: relative;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}
.top-bar .top-inner .top-icon {
  width: 90rpx;
  height: 100rpx;
  position: absolute;
  top: 0;
  left: 0;
  font-size: 40rpx;
}
.top-bar .top-inner .top-icon.icon-home {
  left: 90rpx;
}
.top-bar .top-inner .top-title {
  font-size: 32rpx;
  font-weight: bold;
}

